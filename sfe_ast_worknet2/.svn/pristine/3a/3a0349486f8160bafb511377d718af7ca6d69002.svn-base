/* FingerMultipleSelect (190419, 源��쁺�룄 異붽�) */
.FingerMultipleSelect {
	position: relative;
	display: inline-block;
	width: 180px;
	min-height: 28px;
	font-size: 12.5px;
	border: 1px solid #ccc;
	margin-left: 10px;
	background-color: #fff;
}
.FingerMultipleSelect:before{
	content: '';
	position: absolute;
	width: 1px;
	top: 0;
	bottom: 0;
	right: 32px;
	background-color: #ccc;
}


.FingerMultipleSelect .ms-parent {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	border: 0;
	border-radius: 3px;
	height: 100%;
	min-height: 30px;
	line-height: 30px;
	width: 100% !important;
	background: url("../../../img/select_arrow.png") no-repeat right 12px center;
	background-size: 9px 6px;
}

.FingerMultipleSelect .ms-parent .ms-choice {
	background-color: transparent !important;
	height: 28px;
	border: 0;
}

.FingerMultipleSelect .ms-parent .ms-choice > span {
	right: 32px;
	height: 28px;
	line-height: 28px;
	padding-right: 8px;
}

.FingerMultipleSelect .ms-parent .ms-drop {
	border-top-left-radius: 0;
	border-top-right-radius: 0;
	transform: translateX(-1px) translateY(-2px);
	box-sizing: content-box;
	border-color: #ccc;
	z-index: 1051;
}

.FingerMultipleSelect .ms-parent .ms-drop ul li {
	cursor: pointer;
}

.FingerMultipleSelect .ms-parent .ms-drop input[type="checkbox"] {
	margin-right: 5px;
}
/* FingerMultipleSelect (190419, 源��쁺�룄 異붽�) End. */

