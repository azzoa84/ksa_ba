@charset "utf-8";

/* default layout */
.wf_doc_view {margin:0; padding:0; font-family:'Noto Sans KR',tahoma,AppleGothic,Sans-serif; font-size:13px; line-height:1.2; color:#464646;}
.wf_doc_view * {margin:0; padding:0; border:0;}
.wf_doc_view a {color:#464646; text-decoration:none;}
.wf_doc_view hr, 
.wf_doc_view caption, 
.wf_doc_view legend { display:none; }
.wf_doc_view colgroup { background:none; }
.wf_doc_view h1, 
.wf_doc_view h2, 
.wf_doc_view h3, 
.wf_doc_view h4, 
.wf_doc_view h5, 
.wf_doc_view h6 { font-size:12px; }
.wf_doc_view ul, 
.wf_doc_view ol, 
.wf_doc_view li { list-style:none; }
.wf_doc_view table {clear:both; width:100%; border-collapse:collapse; border-spacing:0; border:0; }
.wf_doc_view p, 
.wf_doc_view div, 
.wf_doc_view th, 
.wf_doc_view td { font-size:13px; }
.wf_doc_view img,
.wf_doc_view input,
.wf_doc_view select,
.wf_doc_view textarea { vertical-align:middle; }
.wf_doc_view input,
.wf_doc_view select,
.wf_doc_view textarea, 
.wf_doc_view button { margin:0; padding:0; font-family:'Nanum Gothic','맑은고딕', dotum, '돋움', Gulim, '굴림', Verdana, sans-serif; font-size:12px; }
.wf_doc_view textarea { resize:none; }
.wf_doc_view input[type="submit"], 
.wf_doc_view button { cursor:pointer; border:0; background:none; }
.wf_doc_view button::-moz-focus-inner{padding: 0; border:0;} /* firefox bug fix */

.wf_doc_view .skip{position:absolute; top:0; left:-1000px; width:0; height:0; font-size:0; line-height:0;}

.wf_doc_view .wf_wrap{width:760px; margin:0 auto; padding:20px 12px 20px 12px;}
/* header */
.wf_doc_view .wf_header {position:relative; padding:0px 0 25px; /*padding:20px 0 25px;*/}
.wf_doc_view .wf_header h1{font-size:24px; text-align:center; float:none;}
.wf_doc_view .wf_header h1 span{display:inline-block; border-bottom:1px solid #000;}
.wf_doc_view .wf_numbering {position:absolute; top:0; right:0; font-size:12px;}

/* footer */
.wf_doc_view .wf_footer{padding:40px 0 20px; text-align:center;}

/* content */
.wf_doc_view .wf_content{overflow:hidden;}
.wf_doc_view .wf_content .wf_left{float:left; margin:0 0 10px; line-height:1.8;}
.wf_doc_view .wf_content .wf_right{float:right; margin:0 0 10px; text-align:right; line-height:1.8;}
.wf_doc_view .wf_separation {padding:20px 0; text-align:center;}

.wf_doc_view .wf_detail-content{font-size:12px; line-height:1.5;}

.wf_doc_view .wf_text_box{min-height:50px; margin:0 0 30px;}
.wf_doc_view .wf_text_box .wf_list_title { margin-bottom:7px; }
.wf_doc_view .wf_text_box.border{padding:20px; border:1px solid #111;}

.wf_doc_view .tbl-authorization{margin:0 0 20px; border:2px solid #111;}
.wf_doc_view .tbl-authorization th{width:10%; border-right:2px solid #111; border-bottom:1px solid #111;}
.wf_doc_view .tbl-authorization td{padding:5px 0; border-bottom:1px solid #111;}
.wf_doc_view .tbl-authorization .item{float:left; min-width:100px; margin:5px 10px; font-size:12px; text-align:center;}
.wf_doc_view .tbl-authorization .item .date { min-height:14px; }
.wf_doc_view .tbl-authorization .sign{height:35px;}
.wf_doc_view .tbl-authorization .sign img{display:block; height:35px; margin:0 auto;}
.wf_doc_view .tbl-authorization .name{min-width:110px; margin:0 0 5px; padding:0 4px 5px; border-bottom:1px solid #aaa;}

.wf_doc_view .tbl-content{margin:0 0 30px; border-top:1px solid #111; border-bottom:1px solid #666;}
.wf_doc_view .tbl-content caption{display:table-caption; padding:0 0 5px; font-size:13px; font-weight:bold; text-align:left;}
.wf_doc_view .tbl-content th{padding:8px; text-align:left; border-bottom:1px solid #666;}
.wf_doc_view .tbl-content td{padding:5px 8px; vertical-align:top;}

.wf_doc_view .wf_file_area{overflow:hidden;}
.wf_doc_view .wf_file_area a{display:block; color:#0e6dd8; text-decoration:underline;}
.wf_doc_view .wf_file_area .wf_attach{float:left; width:49%; margin:0 2% 0 0;}
.wf_doc_view .wf_file_area .wf_refer{float:left; width:49%;}

.wf_doc_view .wf_detail_imagebox img {max-width:500px; max-height:500px;}

@media print {
	.wf_doc_view .wf_file_area a{color:#111; text-decoration:none;}

	.wf_doc_view { page-break-after:always !important; }
	:not(.wf_doc_view) { page-break-after:avoid !important; }
	
	.wf_doc_view .wf_wrap { padding: 0px 10px 0px 10px; }
	.wf_doc_view .wf_footer { padding: 20px 0px 0px 0px; }
	
	thead { display: table-row-group; }
}